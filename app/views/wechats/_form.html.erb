<%= simple_form_for(@wechat) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :to_user_name %>
    <%= f.input :from_user_name %>
    <%= f.input :msg_type %>
    <%= f.input :event %>
    <%= f.input :event_key %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
